{
  "name": "bachelor-work",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "all-install": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "serve": "nodemon ./server/index.js --exec babel-node",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run serve\" \"npm run client\"",
    "lint:js": "eslint --ext .jsx --ext .js client/src server/",
    "lint:sass": "sass-lint --verbose -c .sass-lint.yml",
    "lint": "npm run lint:js && npm run lint:sass"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khrystynapylypets/bachelorWork"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.3",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.4.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.8.6",
    "node": "^8.10.0",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.2",
    "react-bootstrap": "^1.0.0-beta.16",
    "redux-saga": "^1.1.3",
    "sass-lint": "^1.13.1",
    "sudo": "^1.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "concurrently": "^3.5.1",
    "eslint": "^6.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "ws": "3.3.2",
    "husky": "^4.2.5"
  }
}
